# Near-GPU TLC SSD preconditioning 2x random write (4KB)
[global]
rw=randwrite
blocksize=4K
size=100%
ioengine=io_uring
iodepth=${DEPTH}
direct=1
loops=2
group_reporting=1
numjobs=1
percentile_list=1:5:10:20:25:30:40:50:60:70:75:80:90:95:99:99.9:99.95:99.99:99.999:99.9999
